<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE event-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Event 1.1//EN" "http://java.sun.com/dtd/slee-event-jar_1_1.dtd">
<event-jar>
	<description>Event definitions for Radius events</description>
	<library-ref>
		<library-name>radius-provider-library</library-name>
		<library-vendor>org.mobicents.resources.radius</library-vendor>
		<library-version>1.0</library-version>
	</library-ref>
	<!-- Radius connection events -->
	<event-definition>
		<event-type-name>org.mobicents.resources.radius.ACCESS_REQUEST</event-type-name>
		<event-type-vendor>org.mobicents.resources.radius</event-type-vendor>
		<event-type-version>1.0</event-type-version>
		<event-class-name>org.mobicents.resources.radius.RadiusAccessRequest</event-class-name>
	</event-definition>
    <event-definition>
        <event-type-name>org.mobicents.resources.radius.ACCESS_ACK</event-type-name>
        <event-type-vendor>org.mobicents.resources.radius</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.resources.radius.RadiusAccessAck</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.resources.radius.ACCESS_NACK</event-type-name>
        <event-type-vendor>org.mobicents.resources.radius</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.resources.radius.RadiusAccessNack</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.resources.radius.ACCOUNT_START_REQUEST</event-type-name>
        <event-type-vendor>org.mobicents.resources.radius</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.resources.radius.RadiusAccountingRequest</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.resources.radius.ACCOUNT_INTERIM_REQUEST</event-type-name>
        <event-type-vendor>org.mobicents.resources.radius</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.resources.radius.RadiusAccountingRequest</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.resources.radius.ACCOUNT_STOP_REQUEST</event-type-name>
        <event-type-vendor>org.mobicents.resources.radius</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.resources.radius.RadiusAccountingRequest</event-class-name>
    </event-definition>
    <event-definition>
        <event-type-name>org.mobicents.resources.radius.ACCOUNT_RESPONSE</event-type-name>
        <event-type-vendor>org.mobicents.resources.radius</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.resources.radius.RadiusAccountingResponse</event-class-name>
    </event-definition>
</event-jar>
